package org.example;

public class Encaptulation {
    private String name;

    public void setName(String name){
        this.name = name;
    }

    public String getName(){
        return name;
    }

    public static void main(String[] args) {
        Encaptulation User = new Encaptulation() ;
            User.setName ("Farras");
            System.out.println("Nama User: " + User.getName());
    }
}
